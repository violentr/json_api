<div style="padding:20px">
  <%= link_to "Home", root_path %>
</div>

<% if @clients.present? %>
  <h3> Clients list paginated, 1 client per page </h3>
  <hr \>
  <div id="clientsList">
    <ul class="list">
    <% @clients.each do |client| %>
      <li><p class="name"> Name: <%= attributes_for(client,"attributes", "first-name") %> </p></li>
      <li><p class="lastName"> Last Name: <%= attributes_for(client, "attributes", "last-name") %> </p></li>
      <li><p class="primaryPhone"> Primary Phone: <%= attributes_for(client, "attributes", "primary-phone") %> </p>
      <% attributes_for(client, "relationships", "addresses", "data").each do |addr| %>
        <li><p class="addressId"> Address Id: <%= attributes_for(addr, "id") %> </p></li>
        <li><p class="addressType"> Address Type: <%= attributes_for(addr, "type") %> </p></li>
      <% end %>
    <% end %>
    </ul>
    <ul class="pagination"></ul>
  </div>
<% end %>
